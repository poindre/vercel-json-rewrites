{
  "redirects": [
    { "source": "/redirect", "destination": "https://www.google.com" }
  ],
  "rewrites": [
    {
      "source": "/users/:id/",
      "destination": "/users/[id]/"
    },
    {
      "source": "/users/:id/:page/",
      "destination": "/users/[id]/:page/"
    },
    {
      "source": "/users/:id/:page/:blogId/",
      "destination": "/users/[id]/:page/[blogId]/"
    }
  ]
}
