{
  "redirects": [
    { "source": "/redirect", "destination": "https://www.google.com" }
  ],
  "rewrites": [
    {
      "source": "/users/:id/",
      "destination": "/users/[id]/"
    },
    {
      "source": "/users/:id/blogs/",
      "destination": "/users/[id]/blogs/"
    },
    {
      "source": "/users/:id/blogs/:blogId/",
      "destination": "/users/[id]/blogs/[blogId]/"
    }
  ]
}
